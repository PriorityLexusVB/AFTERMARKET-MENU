{
  "name": "priority-lexus-aftermarket-menu",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "dev:emulator": "cross-env VITE_USE_FIRESTORE_EMULATOR=true VITE_FIREBASE_PROJECT_ID=demo-aftermarket-menu npm run dev",
    "build": "vite build",
    "prebuild": "node scripts/validate-env-vars.js",
    "postbuild": "node scripts/validate-build.js",
    "preview": "vite preview",
    "e2e:dev": "cross-env VITE_FORCE_DEMO_MODE=true vite --host 0.0.0.0 --port 4173 --strictPort",
    "e2e:web": "node scripts/run-e2e-webserver.mjs",
    "start": "node index.js",
    "serve": "node index.js",
    "gcp-build": "npm run build",
    "prestart": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate-build": "node scripts/validate-build.js",
    "validate-env": "node scripts/validate-env-vars.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:webkit:smoke": "cross-env PW_WEBKIT=1 playwright test e2e/ipad-fit.spec.ts e2e/presentation.spec.ts",
    "test:e2e:update": "playwright test --update-snapshots",
    "test:e2e:install": "playwright install chromium",
    "typecheck": "tsc --noEmit",
    "healthcheck": "npm run lint && npm run typecheck && npm run test:run && npm run test:e2e",
    "import-data": "tsx scripts/import-mock-data.ts",
    "migrate:feature-positions": "tsx scripts/migrate/add-feature-position-connector.ts",
    "migrate:packages:remove-featureids": "tsx scripts/migrate/remove-package-featureIds.ts",
    "migrate:packages:apply-template": "tsx scripts/migrate/apply-package-template.ts",
    "migrate:backfill-features-from-alacarte": "tsx scripts/migrate/backfill-features-from-alacarte.ts",
    "migrate:alacarte:publish-backfill": "tsx scripts/migrate/backfill-alacarte-isPublished.ts",
    "migrate:onehub:backfill-alacarte": "tsx scripts/migrate/backfill-onehub-alacarte.ts",
    "seed:emulator": "tsx tools/seed-emulator.ts",
    "emulators:start": "firebase emulators:start --only firestore,auth",
    "emulators:wait": "node scripts/wait-for-port.mjs 127.0.0.1 8081 60000",
    "emulators:seed": "npm run emulators:wait && cross-env FIRESTORE_EMULATOR_HOST=127.0.0.1:8081 npm run seed:emulator",
    "firebase:mcp": "npx -y firebase-tools@latest mcp",
    "firestore:fix": "tsx scripts/firestore-fix.ts",
    "check:unicode": "node scripts/check-hidden-unicode.mjs",
    "fix:unicode": "node scripts/check-hidden-unicode.mjs --fix"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.24",
    "express": "^4.22.0",
    "express-rate-limit": "^8.2.1",
    "firebase": "^12.9.0",
    "lucide-react": "^0.563.0",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.19",
    "vite": "^7.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitest/ui": "^4.0.18",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.4",
    "eslint": "^8.57.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "firebase-admin": "^13.6.1",
    "firebase-tools": "^15.5.1",
    "globals": "^15.14.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.1",
    "prettier-eslint": "^16.4.2",
    "tsx": "^4.21.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.14"
  }
}
