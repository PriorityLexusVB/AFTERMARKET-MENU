# All environment variables for this Vite project must start with VITE_.
# Copy this file to .env.local for local development and fill in the values.

# Firebase Configuration
VITE_FIREBASE_API_KEY="YOUR_API_KEY"
VITE_FIREBASE_AUTH_DOMAIN="YOUR_AUTH_DOMAIN"
VITE_FIREBASE_PROJECT_ID="YOUR_PROJECT_ID"
VITE_FIREBASE_STORAGE_BUCKET="YOUR_STORAGE_BUCKET"
VITE_FIREBASE_MESSAGING_SENDER_ID="YOUR_MESSAGING_SENDER_ID"
VITE_FIREBASE_APP_ID="YOUR_APP_ID"

# Local development (optional)
# If true (and in dev), the app will connect to the local Firestore emulator.
VITE_USE_FIRESTORE_EMULATOR="false"

# Firestore emulator location (optional)
VITE_FIRESTORE_EMULATOR_HOST="127.0.0.1"
VITE_FIRESTORE_EMULATOR_PORT="8081"

